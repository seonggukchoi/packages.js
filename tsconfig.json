{
  "compilerOptions": {
    "baseUrl": "./",
    "declaration": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "incremental": true,
    "module": "CommonJS",
    "moduleResolution": "Node",
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "outDir": "./dist",
    "paths": {},
    "removeComments": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "target": "ESNext",
    "types": ["node", "jest"],
  },
  "exclude": ["node_modules", "dist"],
}
